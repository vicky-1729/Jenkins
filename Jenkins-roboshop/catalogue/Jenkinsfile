pipeline {
    agent { label 'AGENT-1' }

    stages {
        stage('Build') {
            steps {
                echo "Building....!"
            }
        }

        stage('Testing') {
            steps {
                echo "Testing...!"
            }
        }
    }

    post {
        success {
            echo "Success..!"
        }
        failure {
            echo "Failure...!"
        }
        always {
            echo "This is roboshop-project"
        }
    }
}
